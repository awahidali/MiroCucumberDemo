<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!--&lt;!&ndash; title of the document &ndash;&gt;
		<documentTitle>HTML Extent</documentTitle>

		&lt;!&ndash; report name - displayed at top-nav &ndash;&gt;
		<reportName>GhChirp Report</reportName>-->

		<!-- title of the document -->
		<documentTitle>Miro - Cucumber Framework</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>Miro - Cucumber Report</reportName>

		<!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
		<dateFormat>dd-MM-yy</dateFormat>

		<!-- global time format override -->   <!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>


		<!-- view report without internet -->
		<offlineMode>false</offlineMode>

		<!-- settings to change component location -->
		<testViewChartLocation>top</testViewChartLocation>
		<chartVisibleOnOpen>true</chartVisibleOnOpen>
		<enableTimeline>true</enableTimeline>
		<disableToggleActionForPassedNode>true
		</disableToggleActionForPassedNode>

		<!-- settings to enable/disable views -->
		<enableCategoryView>true</enableCategoryView>
		<enableAuthorView>true</enableAuthorView>
		<enableExceptionView>true</enableExceptionView>
		<enableTestRunnerLogsView>true</enableTestRunnerLogsView>

		<!-- custom javascript -->
		<js></js>

		<!-- custom styles -->
		<css></css>
	</configuration>
</extentreports>